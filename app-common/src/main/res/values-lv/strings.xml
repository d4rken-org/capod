    <string name="overview_nomaindevice_label">Nav primārās ierīces</string>
    <string name="overview_nomaindevice_description">Visas atklātās ierīces, visticamāk, nav jūsu. Ieslēdziet un pievienojiet ierīci vai pielāgojiet iestatījumus.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth ir atspējots</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth ir atspējots, iespējojiet to ;)</string>

    <string name="permission_bluetooth_connect_label">Bluetooth savienojums</string>
    <string name="permission_bluetooth_connect_description">Šai lietotnei ir nepieciešama \"Bluetooth savienojuma\" atļauja, lai mijiedarbotos ar pārī savienotām ierīcēm un uzsāktu savienojumus.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth skenēšana</string>
    <string name="permission_bluetooth_scan_description">\"Bluetooth skenēšanas\" atļauja ļauj šai lietotnei atklāt un saņemt Bluetooth datus no tuvumā esošām ierīcēm, piemēram, jūsu AirPods.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Šai lietotnei ir nepieciešama \"Bluetooth\" atļauja, lai izveidotu savienojumu ar pārī savienotām Bluetooth ierīcēm.</string>
    <string name="permission_access_fine_location_label">Piekļuve precīzai atrašanās vietai</string>
    <string name="permission_access_fine_location_description">CAPod izmanto \"precīzas atrašanās vietas\" atļauju, lai saņemtu Bluetooth Low Energy datus. Jūsu austiņas izmanto Bluetooth Low Energy tehnoloģiju, lai pārraidītu savu statusu. Šī lietotne NEIZMANTOS Bluetooth datus, lai noteiktu jūsu atrašanās vietu.</string>
    <string name="permission_background_location_label">Piekļuve atrašanās vietai fonā</string>
    <string name="permission_background_location_description">CAPod izmanto \"piekļuvi atrašanās vietai fonā\", lai iespējotu tādas funkcijas kā \"Rādīt uznirstošo logu\" un \"Automātiskais savienojums\", kamēr lietotne ir aizvērta. Piekļuve atrašanās vietai fonā ļauj lietotnei saņemt Bluetooth Low Energy datus, kamēr tā darbojas fonā. Šī lietotne NEIZMANTOS Bluetooth datus, lai noteiktu jūsu atrašanās vietu.</string>
    <string name="permission_ignore_battery_optimizations_label">Atspējot akumulatora optimizāciju</string>
    <string name="permission_ignore_battery_optimizations_description">Akumulatora optimizācija neļauj šai lietotnei uzticami saņemt Bluetooth datus, kamēr lietotne darbojas fonā.</string>
    <string name="permission_required_title">Nepieciešama šāda atļauja:</string>
    <string name="permission_system_alert_window_label">Sistēmas brīdinājuma logs</string>
    <string name="permission_system_alert_window_description">Atļaujiet CAPod zīmēt virs citām lietotnēm, lai padarītu iespējamu funkciju \"Rādīt PopUp\".</string>

