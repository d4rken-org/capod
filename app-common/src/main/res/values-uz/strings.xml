    <string name="overview_nomaindevice_label">Asosiy qurilma yo\'q</string>
    <string name="overview_nomaindevice_description">Aniqlangan barcha qurilmalar sizniki bo\'lishi dargumon. Qurilmangizni yoqing va ulang yoki sozlamalarni o\'zgartiring.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth o\'chirilgan</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth o\'chirilgan, uni yoqing ;)</string>

    <string name="permission_bluetooth_connect_label">Bluetooth ulanishi</string>
    <string name="permission_bluetooth_connect_description">Ushbu ilovaga ulangan qurilmalar bilan ishlash va ulanishlarni boshlash uchun \"Bluetooth ulanishi\" ruxsati kerak.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth skanerlash</string>
    <string name="permission_bluetooth_scan_description">\"Bluetooth skanerlash\" ruxsati ushbu ilovaga AirPods kabi yaqin atrofdagi qurilmalardan Bluetooth ma\'lumotlarini topish va qabul qilish imkonini beradi.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Ushbu ilovaga ulangan bluetooth qurilmalariga ulanish uchun \"Bluetooth\" ruxsati kerak.</string>
    <string name="permission_access_fine_location_label">Aniq joylashuvga kirish</string>
    <string name="permission_access_fine_location_description">CAPod Bluetooth Low Energy ma\'lumotlarini qabul qilish uchun \"aniq joylashuv\" ruxsatidan foydalanadi. Eshitish vositalaringiz o\'z holatini uzatish uchun Bluetooth Low Energy texnologiyasidan foydalanadi. Ushbu ilova joylashuvingizni aniqlash uchun Bluetooth ma\'lumotlaridan foydalanmaydi.</string>
    <string name="permission_background_location_label">Fonda joylashuvga kirish</string>
    <string name="permission_background_location_description">CAPod ilova yopiq bo\'lganda \"Qalqib chiquvchi oynani ko\'rsatish\" va \"Avtomatik ulanish\" kabi funktsiyalarni yoqish uchun \"fonda joylashuvga kirish\" dan foydalanadi. Fonda joylashuvga kirish ilovaga fonda bo\'lganda Bluetooth Low Energy ma\'lumotlarini qabul qilish imkonini beradi. Ushbu ilova joylashuvingizni aniqlash uchun Bluetooth ma\'lumotlaridan foydalanmaydi.</string>
    <string name="permission_ignore_battery_optimizations_label">Batareyani optimallashtirishni o\'chirib qo\'yish</string>
    <string name="permission_ignore_battery_optimizations_description">Batareyani optimallashtirish ushbu ilovaga fonda bo\'lganda Bluetooth ma\'lumotlarini ishonchli tarzda qabul qilishiga to\'sqinlik qiladi.</string>
    <string name="permission_required_title">Quyidagi ruxsat talab qilinadi:</string>
    <string name="permission_system_alert_window_label">Tizim ogohlantirish oynasi</string>
    <string name="permission_system_alert_window_description">CAPodga \"Qalqib chiquvchi oynani ko\'rsatish\" funktsiyasini amalga oshirish uchun boshqa ilovalar ustiga chizishga ruxsat bering.</string>

