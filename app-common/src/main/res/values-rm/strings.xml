    <string name="permission_bluetooth_connect_label">Bluetooth connectar</string>
    <string name="permission_bluetooth_connect_description">Questa app dovra la permissiun "Connectar Bluetooth" per interagir cun apparats cumplexads ed iniziar connexiuns.</string>
    <string name="permission_bluetooth_scan_label">Scannar Bluetooth</string>
    <string name="permission_bluetooth_scan_description">La permissiun "Scannar Bluetooth" permetta a questa app da scuvrir e retschaiver datas da Bluetooth d\'apparats en vischinanza sco Voss AirPods.</string>
