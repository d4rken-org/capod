    <string name="overview_nomaindevice_label">Ingen primær enhed</string>
    <string name="overview_nomaindevice_description">Alle registrerede enheder er sandsynligvis ikke dine. Tænd og tilslut din enhed, eller juster indstillingerne.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth er deaktiveret</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth er deaktiveret, aktiver det ;)</string>

    <string name="permission_bluetooth_connect_label">Bluetooth-forbindelse</string>
    <string name="permission_bluetooth_connect_description">Denne app kræver tilladelsen \"Bluetooth-forbindelse\" for at interagere med parrede enheder og starte forbindelser.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth-scanning</string>
    <string name="permission_bluetooth_scan_description">Tilladelsen \"Bluetooth-scanning\" giver denne app mulighed for at finde og modtage Bluetooth-data fra nærliggende enheder såsom dine AirPods.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Denne app kræver tilladelsen \"Bluetooth\" for at oprette forbindelse til parrede Bluetooth-enheder.</string>
    <string name="permission_access_fine_location_label">Adgang til fin placering</string>
    <string name="permission_access_fine_location_description">CAPod bruger tilladelsen \"fin placering\" til at modtage Bluetooth Low Energy-data. Dine hovedtelefoner bruger Bluetooth Low Energy-teknologi til at udsende deres status. Denne app vil IKKE bruge Bluetooth-data til at bestemme din placering.</string>
    <string name="permission_background_location_label">Adgang til baggrundsplacering</string>
    <string name="permission_background_location_description">CAPod bruger \"adgang til baggrundsplacering\" til at aktivere funktioner som \"Vis pop op\" og \"AutoConnect\", mens appen er lukket. Adgang til baggrundsplacering giver appen mulighed for at modtage Bluetooth Low Energy-data, mens den er i baggrunden. Denne app vil IKKE bruge Bluetooth-data til at bestemme din placering.</string>
    <string name="permission_ignore_battery_optimizations_label">Deaktiver batterioptimeringer</string>
    <string name="permission_ignore_battery_optimizations_description">Batterioptimeringer forhindrer denne app i at modtage Bluetooth-data pålideligt, mens appen er i baggrunden.</string>
    <string name="permission_required_title">Følgende tilladelse er påkrævet:</string>
    <string name="permission_system_alert_window_label">Systemadvarselsvindue</string>
    <string name="permission_system_alert_window_description">Tillad CAPod at tegne oven på andre apps for at gøre funktionen \"Vis PopOp\" mulig.</string>

