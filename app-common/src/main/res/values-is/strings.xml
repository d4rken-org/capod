    <string name="overview_nomaindevice_label">Ekkert aðaltæki</string>
    <string name="overview_nomaindevice_description">Öll skynjuð tæki eru ólíklega þín. Kveiktu á og tengdu tækið þitt eða stilltu stillingarnar.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth er óvirkt</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth er óvirkt, virkjaðu það ;)</string>

    <string name="permission_bluetooth_connect_label">Bluetooth-tenging</string>
    <string name="permission_bluetooth_connect_description">Þetta forrit krefst \"Bluetooth-tenging\" leyfis til að hafa samskipti við pöruð tæki og hefja tengingar.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth-skönnun</string>
    <string name="permission_bluetooth_scan_description">\"Bluetooth-skönnun\" leyfið gerir þessu forriti kleift að finna og taka á móti Bluetooth-gögnum frá nálægum tækjum eins og AirPods þínum.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Þetta forrit krefst \"Bluetooth\" leyfis til að tengjast pöruðum Bluetooth-tækjum.</string>
    <string name="permission_access_fine_location_label">Aðgangur að nákvæmri staðsetningu</string>
    <string name="permission_access_fine_location_description">CAPod notar \"nákvæma staðsetningu\" leyfið til að taka á móti Bluetooth Low Energy gögnum. Heyrnartólin þín nota Bluetooth Low Energy tækni til að senda út stöðu sína. Þetta forrit mun EKKI nota Bluetooth-gögn til að ákvarða staðsetningu þína.</string>
    <string name="permission_background_location_label">Aðgangur að staðsetningu í bakgrunni</string>
    <string name="permission_background_location_description">CAPod notar \"aðgang að staðsetningu í bakgrunni\" til að virkja eiginleika eins og \"Sýna sprettiglugga\" og \"Sjálfvirk tenging\" á meðan forritið er lokað. Aðgangur að staðsetningu í bakgrunni gerir forritinu kleift að taka á móti Bluetooth Low Energy gögnum á meðan það er í bakgrunni. Þetta forrit mun EKKI nota Bluetooth-gögn til að ákvarða staðsetningu þína.</string>
    <string name="permission_ignore_battery_optimizations_label">Slökkva á rafhlöðubestun</string>
    <string name="permission_ignore_battery_optimizations_description">Rafhlöðubestun kemur í veg fyrir að þetta forrit geti áreiðanlega tekið á móti Bluetooth-gögnum á meðan forritið er í bakgrunni.</string>
    <string name="permission_required_title">Eftirfarandi leyfi er krafist:</string>
    <string name="permission_system_alert_window_label">Kerfisviðvörunargluggi</string>
    <string name="permission_system_alert_window_description">Leyfðu CAPod að teikna yfir önnur forrit til að gera eiginleikann \"Sýna PopUp\" mögulegan.</string>

