    <string name="overview_nomaindevice_label">Hakuna kifaa msingi</string>
    <string name="overview_nomaindevice_description">Vifaa vyote vilivyogunduliwa haviwezekani kuwa vyako. Washa na uunganishe kifaa chako au urekebishe mipangilio.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth imezimwa</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth imezimwa, iwashe ;)</string>
    <string name="permission_bluetooth_connect_label">Unganisha Bluetooth</string>
    <string name="permission_bluetooth_connect_description">Programu hii inahitaji ruhusa ya \"Unganisha Bluetooth\" ili kuingiliana na vifaa vilivyooanishwa na kuanzisha miunganisho.</string>
    <string name="permission_bluetooth_scan_label">Inaskani Bluetooth</string>
    <string name="permission_bluetooth_scan_description">Ruhusa ya \"Inaskani Bluetooth\" inaruhusu programu hii kugundua na kupokea data ya Bluetooth kutoka kwa vifaa vilivyo karibu kama vile AirPods zako.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Programu hii inahitaji ruhusa ya \"Bluetooth\" ili kuunganisha kwa vifaa vya Bluetooth vilivyooanishwa.</string>
    <string name="permission_access_fine_location_label">Fikia eneo bora</string>
    <string name="permission_access_fine_location_description">CAPod hutumia ruhusa ya \"eneo bora\" kupokea data ya Bluetooth Low Energy. Vipokea sauti vyako vinavyotumia teknolojia ya Bluetooth Low Energy kutangaza hali yake. Programu hii haitatumia data ya Bluetooth kubaini eneo lako.</string>
    <string name="permission_background_location_label">Ufikiaji wa eneo la usuli</string>
    <string name="permission_background_location_description">CAPod hutumia \"ufikiaji wa eneo la usuli\" kuwezesha vipengele kama vile \"Onyesha kidirisha\" na \"Unganisha kiotomatiki\" wakati programu imefungwa. Ufikiaji wa eneo la usuli huruhusu programu kupokea data ya Bluetooth Hemat Energi wakati iko nyuma. Programu hii haitatumia data ya Bluetooth kubaini eneo lako.</string>
    <string name="permission_ignore_battery_optimizations_label">Zima uboreshaji wa betri</string>
    <string name="permission_ignore_battery_optimizations_description">Uboreshaji wa betri huzuia programu hii kupokea data ya Bluetooth kwa uhakika programu ikiwa nyuma.</string>
