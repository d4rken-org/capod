    <string name="overview_nomaindevice_label">Ez dago gailu nagusirik</string>
    <string name="overview_nomaindevice_description">Detektatutako gailu guztiak nekez izango dira zureak. Piztu eta konektatu zure gailua edo egokitu ezarpenak.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth desgaituta dago</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth desgaituta dago, gaitu ;)</string>

    <string name="permission_bluetooth_connect_label">Bluetooth konexioa</string>
    <string name="permission_bluetooth_connect_description">Aplikazio honek \"Bluetooth konexioa\" baimena behar du parekatutako gailuekin elkarreragiteko eta konexioak hasteko.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth eskaneatzea</string>
    <string name="permission_bluetooth_scan_description">\"Bluetooth eskaneatzea\" baimenak aplikazio honi zure AirPods bezalako inguruko gailuetatik Bluetooth datuak aurkitu eta jasotzeko aukera ematen dio.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Aplikazio honek \"Bluetooth\" baimena behar du parekatutako Bluetooth gailuetara konektatzeko.</string>
    <string name="permission_access_fine_location_label">Sarbide zehatza kokapenera</string>
    <string name="permission_access_fine_location_description">CAPod-ek \"kokapen zehatza\" baimena erabiltzen du Bluetooth Energia Baxuko datuak jasotzeko. Zure entzungailuek Bluetooth Energia Baxuko teknologia erabiltzen dute beren egoera igortzeko. Aplikazio honek EZ du Bluetooth datuak erabiliko zure kokapena zehazteko.</string>
    <string name="permission_background_location_label">Atzeko planoko kokapen sarbidea</string>
    <string name="permission_background_location_description">CAPod-ek \"atzeko planoko kokapen sarbidea\" erabiltzen du aplikazioa itxita dagoen bitartean \"Erakutsi pop-up\" eta \"Automatikoki konektatu\" bezalako funtzioak gaitzeko. Atzeko planoko kokapen sarbideak aplikazioari Bluetooth Energia Baxuko datuak jasotzeko aukera ematen dio atzeko planoan dagoen bitartean. Aplikazio honek EZ du Bluetooth datuak erabiliko zure kokapena zehazteko.</string>
    <string name="permission_ignore_battery_optimizations_label">Desgaitu bateriaren optimizazioak</string>
    <string name="permission_ignore_battery_optimizations_description">Bateriaren optimizazioek aplikazio honek atzeko planoan dagoen bitartean Bluetooth datuak modu fidagarrian jasotzea eragozten dute.</string>
