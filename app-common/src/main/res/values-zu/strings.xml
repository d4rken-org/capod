    <string name="overview_nomaindevice_label">Ayikho idivayisi eyinhloko</string>
    <string name="overview_nomaindevice_description">Wonke amadivayisi atholiwe mancane amathuba okuthi awakho. Vula futhi uxhume idivayisi yakho noma ulungise izilungiselelo.</string>
    <string name="overview_bluetooth_disabled_label">I-Bluetooth ikhubaziwe</string>
    <string name="overview_bluetooth_disabled_description">I-Bluetooth ikhubaziwe, yivule ;)</string>

    <string name="permission_bluetooth_connect_label">Ukuxhuma kwe-Bluetooth</string>
    <string name="permission_bluetooth_connect_description">Lolu hlelo lokusebenza ludinga imvume ethi \"Ukuxhuma kwe-Bluetooth\" ukuze lusebenzisane namadivayisi abhanqiwe futhi luqale ukuxhumana.</string>
    <string name="permission_bluetooth_scan_label">Ukuskena kwe-Bluetooth</string>
    <string name="permission_bluetooth_scan_description">Imvume \"Ukuskena kwe-Bluetooth\" ivumela lolu hlelo lokusebenza ukuthola nokwamukela idatha ye-Bluetooth kumadivayisi aseduze njengama-AirPods akho.</string>
    <string name="permission_bluetooth_label">I-Bluetooth</string>
    <string name="permission_bluetooth_description">Lolu hlelo lokusebenza ludinga imvume ye-\"Bluetooth\" ukuze luxhumeke kumadivayisi e-bluetooth abhanqiwe.</string>
    <string name="permission_access_fine_location_label">Ukufinyelela indawo eqondile</string>
    <string name="permission_access_fine_location_description">I-CAPod isebenzisa imvume \"yendawo eqondile\" ukuze yamukele idatha ye-Bluetooth Low Energy. Amahedfoni akho asebenzisa ubuchwepheshe be-Bluetooth Low Energy ukusakaza isimo sawo. Lolu hlelo lokusebenza NGEKE lusebenzise idatha ye-Bluetooth ukunquma indawo yakho.</string>
    <string name="permission_background_location_label">Ukufinyelela indawo engemuva</string>
    <string name="permission_background_location_description">I-CAPod isebenzisa \"ukufinyelela indawo engemuva\" ukuze inike amandla izici ezifana ne-\"Bonisa isigelekeqe\" kanye ne-\"AutoConnect\" ngenkathi uhlelo lokusebenza luvaliwe. Ukufinyelela indawo engemuva kuvumela uhlelo lokusebenza ukwamukela idatha ye-Bluetooth Low Energy ngenkathi lusengemuva. Lolu hlelo lokusebenza NGEKE lusebenzise idatha ye-Bluetooth ukunquma indawo yakho.</string>
    <string name="permission_ignore_battery_optimizations_label">Khubaza ukuthuthukiswa kwebhethri</string>
    <string name="permission_ignore_battery_optimizations_description">Ukuthuthukiswa kwebhethri kuvimbela lolu hlelo lokusebenza ekwamukeleni idatha ye-Bluetooth ngokuthembekile ngenkathi uhlelo lokusebenza lusengemuva.</string>
    <string name="permission_required_title">Kudingeka le mvume elandelayo:</string>
    <string name="permission_system_alert_window_label">Iwindi Lesexwayiso Lesistimu</string>
    <string name="permission_system_alert_window_description">Vumela i-CAPod ukuthi idwebe ngaphezu kwezinye izinhlelo zokusebenza ukuze kwenzeke isici esithi \"Bonisa i-PopUp\".</string>

