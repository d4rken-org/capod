    <string name="permission_system_alert_window_label">Dritarja e Alarmit të Sistemit</string>
    <string name="permission_system_alert_window_description">Lejoni CAPod të vizatojë mbi aplikacione të tjera për të bërë të mundur veçorinë "Shfaq PopUp".</string>

