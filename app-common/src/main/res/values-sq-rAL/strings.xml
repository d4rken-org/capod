    <string name="overview_nomaindevice_label">Asnjë pajisje kryesore</string>
    <string name="overview_nomaindevice_description">Të gjitha pajisjet e zbuluara nuk ka gjasa të jenë tuajat. Ndizni dhe lidhni pajisjen tuaj ose rregulloni cilësimet.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth është i çaktivizuar</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth është i çaktivizuar, aktivizojeni ;)</string>

    <string name="permission_bluetooth_connect_label">Lidhja Bluetooth</string>
    <string name="permission_bluetooth_connect_description">Ky aplikacion kërkon lejen "Lidhja Bluetooth" për të ndërvepruar me pajisjet e çiftuara dhe për të nisur lidhjet.</string>
    <string name="permission_bluetooth_scan_label">Skanimi Bluetooth</string>
    <string name="permission_bluetooth_scan_description">Leja "Skanimi Bluetooth" i lejon këtij aplikacioni të zbulojë dhe të marrë të dhëna Bluetooth nga pajisjet e afërta si AirPods tuaj.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Ky aplikacion kërkon lejen "Bluetooth" për t\'u lidhur me pajisjet e çiftuara bluetooth.</string>
    <string name="permission_access_fine_location_label">Qasja në vendndodhjen e saktë</string>
    <string name="permission_access_fine_location_description">CAPod përdor lejen "vendndodhja e saktë" për të marrë të dhëna Bluetooth Low Energy. Kufjet tuaja përdorin teknologjinë Bluetooth Low Energy për të transmetuar statusin e tyre. Ky aplikacion NUK do të përdorë të dhënat Bluetooth për të përcaktuar vendndodhjen tuaj.</string>
    <string name="permission_background_location_label">Qasja në vendndodhjen në sfond</string>
    <string name="permission_background_location_description">CAPod përdor "qasjen në vendndodhjen në sfond" për të aktivizuar veçori si "Shfaq popup" dhe "AutoConnect" ndërsa aplikacioni është i mbyllur. Qasja në vendndodhjen në sfond i lejon aplikacionit të marrë të dhëna Bluetooth Low Energy ndërsa është në sfond. Ky aplikacion NUK do të përdorë të dhënat Bluetooth për të përcaktuar vendndodhjen tuaj.</string>
    <string name="permission_ignore_battery_optimizations_label">Çaktivizo optimizimet e baterisë</string>
    <string name="permission_ignore_battery_optimizations_description">Optimizimet e baterisë e pengojnë këtë aplikacion të marrë në mënyrë të besueshme të dhëna Bluetooth ndërsa aplikacioni është në sfond.</string>
