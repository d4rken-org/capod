    <string name="overview_nomaindevice_label">Nema primarnog uređaja</string>
    <string name="overview_nomaindevice_description">Svi otkriveni uređaji vjerojatno nisu vaši. Uključite i povežite svoj uređaj ili prilagodite postavke.</string>
    <string name="overview_bluetooth_disabled_label">Bluetooth je onemogućen</string>
    <string name="overview_bluetooth_disabled_description">Bluetooth je onemogućen, omogućite ga ;)</string>
    <string name="permission_bluetooth_connect_label">Bluetooth povezivanje</string>
    <string name="permission_bluetooth_connect_description">Ova aplikacija zahtijeva dopuštenje \"Bluetooth povezivanje\" za interakciju s uparenim uređajima i pokretanje veza.</string>
    <string name="permission_bluetooth_scan_label">Bluetooth skeniranje</string>
    <string name="permission_bluetooth_scan_description">Dopuštenje \"Bluetooth skeniranje\" omogućuje ovoj aplikaciji otkrivanje i primanje Bluetooth podataka s obližnjih uređaja kao što su vaši AirPods.</string>
    <string name="permission_bluetooth_label">Bluetooth</string>
    <string name="permission_bluetooth_description">Ova aplikacija zahtijeva dopuštenje \"Bluetooth\" za povezivanje s uparenim Bluetooth uređajima.</string>
    <string name="permission_access_fine_location_label">Pristup preciznoj lokaciji</string>
    <string name="permission_access_fine_location_description">CAPod koristi dopuštenje \"precizna lokacija\" za primanje podataka Bluetooth Low Energy. Vaše slušalice koriste tehnologiju Bluetooth Low Energy za emitiranje svog statusa. Ova aplikacija NEĆE koristiti Bluetooth podatke za određivanje vaše lokacije.</string>
    <string name="permission_background_location_label">Pristup lokaciji u pozadini</string>
    <string name="permission_background_location_description">CAPod koristi \"pristup lokaciji u pozadini\" kako bi omogućio značajke kao što su \"Prikaži skočni prozor\" i \"Automatsko povezivanje\" dok je aplikacija zatvorena. Pristup lokaciji u pozadini omogućuje aplikaciji primanje podataka Bluetooth Low Energy dok je u pozadini. Ova aplikacija NEĆE koristiti Bluetooth podatke za određivanje vaše lokacije.</string>
    <string name="permission_ignore_battery_optimizations_label">Onemogući optimizaciju baterije</string>
    <string name="permission_ignore_battery_optimizations_description">Optimizacija baterije sprječava ovu aplikaciju da pouzdano prima Bluetooth podatke dok je aplikacija u pozadini.</string>
    <string name="permission_required_title">Potrebno je sljedeće dopuštenje:</string>
    <string name="permission_system_alert_window_label">Prozor sistemskog upozorenja</string>
    <string name="permission_system_alert_window_description">Dopustite CAPod-u da se prikazuje preko drugih aplikacija kako bi značajka \"Prikaži PopUp\" bila moguća.</string>
