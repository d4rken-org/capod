    <string name="widget_config_background_color_label">பின்னணி நிறம்</string>
    <string name="widget_config_foreground_color_label">உரை மற்றும் சின்ன வண்ணம்</string>
    <string name="widget_config_transparency_label">பின்னணி வெளிப்படையாக்கம்</string>
